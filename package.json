{
	"name": "svelte-shadcn-cms-builder",
	"version": "0.0.1",
	"description": "Svelte Shadcn CMS website builder",
	"author": "Jujoco",
	"repository": {
		"type": "git",
		"url": "https://github.com/jujoco/svelte-shadcn-cms-builder.git"
	},
	"keywords": [
		"svelte",
		"cms"
	],
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
